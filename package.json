  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:prod": "NODE_ENV=production next dev",
    "dev:production": "./scripts/dev-production.sh",
    "test:production": "node scripts/test-production.js",
    "test:fixes": "node scripts/test-production-fixes.js",
    "verify:fixes": "node scripts/verify-fixes.js",
    "build:analyze": "ANALYZE=true next build",
    "build:debug": "DEBUG=true next build",
    "build:production": "NODE_ENV=production next build",
    "build:safe": "NODE_ENV=production next build --config next.config.production.js",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "pre-production-check": "npm run type-check && npm run lint && npm run build:safe",
    "start:local-prod": "npm run build:production && npm run start",
    "ubuntu:setup": "chmod +x scripts/setup-ubuntu.sh && ./scripts/setup-ubuntu.sh",
    "ubuntu:deploy": "chmod +x scripts/deploy-ubuntu.sh && ./scripts/deploy-ubuntu.sh",
    "ubuntu:quick": "chmod +x scripts/quick-deploy.sh && ./scripts/quick-deploy.sh",
    "ubuntu:monitor": "chmod +x scripts/monitor.sh && ./scripts/monitor.sh",
    "ubuntu:status": "pm2 status && sudo systemctl status nginx cloudflare-tunnel"
